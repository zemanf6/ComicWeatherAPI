<Window x:Class="ComicWeatherApi.Suninfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComicWeatherApi"
        mc:Ignorable="d" FontSize="22"
        Title="Suninfo" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <Button x:Name="LoadSunInfo" Margin="20" Click="LoadSunInfo_Click">Load sun info</Button>
            <TextBlock x:Name="SunriseText" Margin="20" />
            <TextBlock x:Name="SunsetText" Margin="20" />
        </StackPanel>
    </Grid>
</Window>
